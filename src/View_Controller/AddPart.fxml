<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.AddPartController">
   <children>
      <Label layoutX="38.0" layoutY="23.0" text="Add Part" textFill="DARKBLUE">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <RadioButton fx:id="inHouseRadioButton" layoutX="167.0" layoutY="39.0" mnemonicParsing="false" text="In-House">
         <font>
            <Font size="16.0" />
         </font></RadioButton>
      <RadioButton fx:id="outSourcedRadioButton" layoutX="296.0" layoutY="39.0" mnemonicParsing="false" text="Outsourced">
         <font>
            <Font size="16.0" />
         </font></RadioButton>
      <VBox layoutX="187.0" layoutY="84.0" prefHeight="322.0" prefWidth="189.0" spacing="20.0">
         <children>
            <TextField fx:id="partIDTextField">
               <font>
                  <Font size="16.0" />
               </font></TextField>
            <TextField fx:id="partNameTextField" promptText="Part Name">
               <font>
                  <Font size="16.0" />
               </font></TextField>
            <TextField fx:id="partInvTextField" promptText="Inv">
               <font>
                  <Font size="16.0" />
               </font></TextField>
            <TextField fx:id="priceTextField" promptText="Price/Cost">
               <font>
                  <Font size="16.0" />
               </font></TextField>
            <TextField fx:id="maxPartTextField" maxWidth="86.0" prefWidth="86.0" promptText="Max">
               <font>
                  <Font size="16.0" />
               </font></TextField>
            <TextField fx:id="companyNameTextField" prefHeight="54.0" prefWidth="201.0" promptText="Company Name">
               <font>
                  <Font size="16.0" />
               </font></TextField>
         </children>
      </VBox>
      <VBox layoutX="38.0" layoutY="84.0" prefHeight="322.0" prefWidth="134.0" spacing="32.0">
         <children>
            <Label text="ID">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label text="Name">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label text="Inv">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label text="Price/Cost">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label text="Max">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label lineSpacing="15.0" text="Company Name">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <Label layoutX="387.0" layoutY="318.0" text="Min">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="minPartTextField" layoutX="424.0" layoutY="313.0" maxWidth="86.0" prefHeight="25.0" prefWidth="86.0" promptText="Min">
         <font>
            <Font size="16.0" />
         </font></TextField>
      <Button fx:id="addPartSaveButton" layoutX="329.0" layoutY="471.0" mnemonicParsing="false" style="-fx-border-color: black;" text="Save">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="addPartCancelButton" layoutX="410.0" layoutY="471.0" mnemonicParsing="false" onAction="#openMainScreen" style="-fx-border-color: black;" text="Cancel">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
